alias cov='find ./ -type f -exec dos2unix {} \;'
alias fa='find ./ -type f | xargs grep -ns'
alias fc='find ./ -name \*.c | xargs grep -ns'
alias fh='find ./ -name \*.h | xargs grep -ns'
alias fs='find ./ -name \*.c -o -name \*.h -o -name \*.s -o -name \*.S | xargs grep -ns'
alias indent='indent -npcs -npro -kr -i8 -ts8 -sob -l80 -ss -ncs -cp1'
alias ll='ls -l'
alias ls='ls -F --color=auto --show-control-chars'
alias wcl='find ./ -name \*.c -o -name \*.h | xargs wc -l'
alias rm='rm -rf'
alias gdb='gdb-multiarch'
alias cls='printf "\033c"'

